    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "path": "^0.12.7",
    "superagent": "^5.3.1",
    "supertest": "^4.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
